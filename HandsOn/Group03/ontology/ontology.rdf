<?xml version ="1.0"?>
  <!DOCTYPE rdf:RDF [
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY pollutionSensor "http://www.airqualitymadrid.es/ontologies/pollutionSensor#" >
    <!ENTITY pollutantType "http://www.airqualitymadrid.es/ontologies/pollutantType#" >
    <!ENTITY measurement "http://www.airqualitymadrid.es/ontologies/measurement#" >
  ]>

  <rdf:RDF xmlns="http://www.airqualitymadrid.es/ontologies/pollutionSensor"
    xml:base="http://www.airqualitymadrid.es/ontologies/pollutionSensor"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:pollutionSensor="http://www.airqualitymadrid.es/ontologies/pollutionSensor#"
    xmlns:pollutantType="http://www.airqualitymadrid.es/ontologies/pollutantType#"
    xmlns:measurement="http://www.airqualitymadrid.es/ontologies/measurement#">

    <rdfs:Property rdf:about="&pollutionSensor;hasElevation">
      <rdfs:range rdf:resource="&rdfs;Literal"/>
      <rdfs:domain rdf:resource="#pollutionSensor"/>
    </rdfs:Property>

    <rdfs:Property rdf:about="&pollutionSensor;hasId">
      <rdfs:range rdf:resource="&rdfs;Literal"/>
      <rdfs:domain rdf:resource="#pollutionSensor"/>
    </rdfs:Property>

    <rdfs:Property rdf:about="&pollutionSensor;hasName">
      <rdfs:range rdf:resource="&rdfs;Literal"/>
      <rdfs:domain rdf:resource="#pollutionSensor"/>
    </rdfs:Property>

    <rdfs:Property rdf:about="&pollutionSensor;hasLongitude">
      <rdfs:range rdf:resource="&rdfs;Literal"/>
      <rdfs:domain rdf:resource="#pollutionSensor"/>
    </rdfs:Property>

    <rdfs:Property rdf:about="&pollutionSensor;hasLatitude">
      <rdfs:range rdf:resource="&rdfs;Literal"/>
      <rdfs:domain rdf:resource="#pollutionSensor"/>
    </rdfs:Property>

    <rdfs:Property rdf:about="&measurement;hasDate">
      <rdfs:range rdf:resource="&rdfs;Literal"/>
      <rdfs:domain rdf:resource="#measurement"/>
    </rdfs:Property>

    <rdfs:Property rdf:about="&measurement;hasPollutant">
      <rdfs:range rdf:resource="&rdfs;Literal"/>
      <rdfs:domain rdf:resource="#measurement"/>
    </rdfs:Property>

    <rdfs:Property rdf:about="&pollutantType;hasValue">
      <rdfs:range rdf:resource="&rdfs;Literal"/>
      <rdfs:domain rdf:resource="#pollutantType"/>
    </rdfs:Property>

    <rdfs:Class rdf:about="#Measurement"/>

    <rdfs:Class rdf:about="#PollutantType"/>

    <rdfs:Class rdf:about="#PollutionSensor"/>

    <rdfs:Class rdf:about="#PollutantBEN">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantCH4">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantCO">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantEBE">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantNMHC">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantNO">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantNO_2">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantNOx">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantO_3">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantPM10">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantPM25">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantSO_2">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantTCH">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="#PollutantTOL">
      <rdfs:subClassOf rdf:resource="#pollutantType"/>
    </rdfs:Class>

</rdf:RDF>
